
<web-app>
  <display-name>Iron Man Web Application</display-name>
  
  <servlet>
  	<servlet-name>ironManDispatcher</servlet-name>
  	<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ironManDispatcher</servlet-name>
  	<url-pattern>*.html</url-pattern>
  </servlet-mapping>
</web-app>
